{
  "preset": "google",
  "maximumLineLength": 120,
  "fileExtensions": [ ".js",".spec.js", "jscs" ],
  "disallowKeywordsOnNewLine": [],
  "requireKeywordsOnNewLine": ["else"],
  "disallowMultipleVarDecl": "exceptUndefined",
  "requireSpaceAfterKeywords": [
    "else",
    "for",
    "while",
    "do",
    "switch",
    "return",
    "try",
    "catch"
  ],
  "disallowSpaceAfterKeywords": [
    "if"
  ]
}